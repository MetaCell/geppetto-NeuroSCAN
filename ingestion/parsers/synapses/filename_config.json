{
  "directory": "../../data/pre_synapses",
  "extension": [".obj"],
  "regex": {
    "id":{
      "expression": "_([^_]*)(chemical|electrical)([^_]*)_",
      "match": "$1-$3"
    },
    "name":{
      "expression": "_([^_]*)(chemical|electrical)([^_]*)_",
      "match": "$1-$3"
    },
    "pre":{
      "expression": "_([^_]*)(chemical|electrical)([^_]*)_",
      "match": "$1"
    },
    "post":{
      "expression": "_([^_]*)(chemical|electrical)([^_]*)_",
      "match": "$3"
    },
    "type":{
      "expression": "_([^_]*)(chemical|electrical)([^_]*)_",
      "match": "$2"
    }
  }
}
