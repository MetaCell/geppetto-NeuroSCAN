{
  "parser": "file",
  "directory": "../../../data/neuroscan/",
  "include": ".*/\\w*/\\d*/synapses/\\w*[PostSyn|PreSyn]/.*\\.obj",
  "regex": {
    "id":{
      "expression": ".*/(\\d*)/synapses/(.+)/(.+)\\.obj",
      "match": "$1_$2_$3"
    },
    "uid":{
      "expression": ".*/(\\d*)/synapses/(.+)/(.+)\\.obj",
      "match": "$1_$2_$3"
    },
    "timepoint": {
      "expression": ".*/\\w*/(\\d*)/synapses/\\w*[PostSyn|PreSyn]/.*\\.obj",
      "match": "$1"
    },
    "type": {
      "expression": ".+/.+(electrical|chemical).+\\.obj",
      "match": "$1"
    },
    "section": {
      "expression": ".+/.+_((:?certain|uncertain).+?)_.+\\.obj",
      "match": "$1"
    },
    "zs": {
      "expression": ".+/.+zs(\\d*).+\\.obj",
      "match": "$1"
    },
    "position": {
      "expression": ".*/\\w*/\\d*/synapses/\\w*(Post|Pre)Syn/.*\\.obj",
      "match": "$1"
    },
    "neuronSite": {
      "expression": ".*-(\\d*)\\.obj",
      "match": "$1"
    },
    "neuronPre": {
      "expression": "_(\\w*?)(?:chemical|electrical).+\\.obj",
      "match": "$1"
    },
    "neuronPost": {
      "expression": ".*_.+(?:chemical|electrical)(.+?(:?&.+?)*?)_.*\\.obj",
      "match": "$1"
    },
    "postNeuron": {
      "expression": ".*/\\w*/\\d*/synapses/(.*)_[PostSyn|PreSyn]*/.*\\.obj",
      "match": "$1"
    }
  },
  "split": {
    "expression": ".*/neuroscan/\\w*/(\\d*)/.*",
    "match": "$1"
  }
}
