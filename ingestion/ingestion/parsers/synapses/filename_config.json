{
  "parser": "file",
  "directory": "../../../data/neuroscan/",
  "include": ".*/\\w*/\\d*/synapses/.*\\.gltf",
  "regex": {
    "id":{
      "expression": ".*/(\\d*)/synapses/(.+)\\.gltf",
      "match": "$1_$2"
    },
    "uid":{
      "expression": ".*/(\\d*)/synapses/(.+)\\.gltf",
      "match": "$2"
    },
    "timepoint": {
      "expression": ".*/\\w*/(\\d*)/synapses/.*\\.gltf",
      "match": "$1"
    },
    "type": {
      "expression": ".+/.+(electrical|chemical|undefined).+\\.gltf",
      "match": "$1"
    },
    "section": {
      "expression": ".*/.*\\-(.*)_.*\\.gltf",
      "match": "$1"
    },
    "zs": {
      "expression": ".+/.+zs(\\d*).+\\.gltf",
      "match": "$1"
    },
    "position": {
      "expression": ".*/\\w*/\\d*/synapses/.*(post|pre).*\\.gltf",
      "match": "$1"
    },
    "neuronSite": {
      "expression": ".*/\\w*/\\d*/synapses/.*[post|pre](\\d*)\\.gltf",
      "match": "$1"
    },
    "neuronPre": {
      "expression": "_(.*?)(?:chemical|electrical|undefined).+\\.gltf",
      "match": "$1"
    },
    "neuronPost": {
      "expression": ".*_.*(?:chemical|electrical|undefined)(.+?(:?_.+?)*?)-[A-Z].*\\.gltf",
      "match": "$1"
    },
    "postNeuron": {
      "expression": ".*/\\w*/\\d*/synapses/(.*)[post|pre]*/.*\\.gltf",
      "match": "$1"
    }
  },
  "split": {
    "expression": ".*/neuroscan/\\w*/(\\d*)/.*",
    "match": "$1"
  }
}
