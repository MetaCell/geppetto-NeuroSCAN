{
  "parser": "file",
  "directory": "../../../data/neuroscan/",
  "include": ".*/\\w*/\\d*/contacts/.*\\.gltf",
  "regex": {
    "id":{
      "expression": ".*/(\\d*)/contacts/.*_(\\w*?)by(\\w*?)\\.gltf",
      "match": "$1-$2-$3"
    },
    "uid":{
      "expression": ".*/(\\d*)/contacts/.*_(\\w*?)by(\\w*?)\\.gltf",
      "match": "$2-$3"
    },
    "neuronA":{
      "expression": "_(.+?)by",
      "match": "$1"
    },
    "neuronB":{
      "expression": "_.+?by(.+?)\\.gltf",
      "match": "$1"
    },
    "timepoint": {
      "expression": ".*/\\w*/(\\d*)/contacts/.*\\.gltf",
      "match": "$1"
    }
  },
  "split": {
    "expression": ".*/(\\d*)/.*",
    "match": "$1"
  }
}
