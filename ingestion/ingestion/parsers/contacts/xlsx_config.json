{
  "parser": "xls",
  "directory": "../../../data/neuroscan/",
  "include": ".*/\\w*/\\d*/contacts.xlsx*",
  "sheet_name": 0,
  "column_id": "${Neuron1}-${Neuron2}",
  "column_of_interest": {
    "${Neuron1}-${Neuron2}": "uid",
    "Neuron1": "neuronA",
    "Neuron2": "neuronB",
    "Weight": "weight"
  },
  "split": {
    "expression": ".*/neuroscan/\\w*/(\\d*)/.*",
    "match": "$1"
  }
}
