{
  "parser": "file",
  "directory": "../../../data/neuroscan/",
  "include": ".*/\\w*/\\d*/neurons/\\w*\\.gltf",
  "regex": {
    "id":{
      "expression": ".*\\/\\w*\\/(\\d*)\\/neurons\\/.*_(.*)\\.gltf",
      "match": "$1_$2"
    },
    "uid":{
      "expression": "_(.*)\\.gltf",
      "match": "$1"
    },
    "timepoint": {
      "expression": ".*\\/\\w*\\/(\\d*)\\/neurons\\/\\w*\\.gltf",
      "match": "$1"
    }
  }
}
